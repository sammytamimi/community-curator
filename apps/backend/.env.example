# .env file

# Shared Azure Configuration
AZURE_OPENAI_API_KEY="your_azure_openai_api_key_here"
AZURE_OPENAI_API_VERSION="2025-01-01-preview"

# --- Model-Specific Deployments ---

# 1. For the Embedding Model
AZURE_OPENAI_ENDPOINT_EMBEDDING="your_embedding_endpoint_here"
AZURE_OPENAI_EMBEDDING_DEPLOYMENT_NAME="text-embedding-3-small"

# 2. For the Chat/Completions Model
AZURE_OPENAI_ENDPOINT_CHAT="your_chat_endpoint_here"
AZURE_OPENAI_CHAT_DEPLOYMENT_NAME="gpt-4o"

# --- LangSmith Tracing (Optional) ---
LANGSMITH_TRACING="true"
LANGSMITH_API_KEY="your_langsmith_api_key_here"
